<div class="container">
  <div class="row">

    <%= render 'users/user_info', book: @book %>

    <div class="col-md-9">
      <h2>Users</h2>
      <table>
        <tr>
          <th>image</th>
          <th>name</th>
          <br>
        </tr>
        <% @users.each do |user| %>
          <tr>
            <td><%= image_tag user.get_image %></td>
            <td><%= user.name %></td>
            <td><%= link to 'Show', user_path(user.id) %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>